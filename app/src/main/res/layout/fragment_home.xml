<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C0C0C0">

    <TextView
        android:id="@+id/home_page_title2"
        android:layout_width="96dp"
        android:layout_height="53dp"
        android:layout_marginLeft="20dp"
        android:elevation="10dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="left"
        android:lineSpacingExtra="-7dp"
        android:text="GO"
        android:layout_marginTop="700dp"
        app:layout_constraintBottom_toTopOf="@id/auto_nav"
        android:textColor="#000000"
        android:textSize="40sp"
        android:typeface="sans"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#e2c239"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/auto_nav2"
        android:layout_width="109dp"
        android:layout_height="109dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="184dp"
        android:background="@drawable/round_button"
        android:onClick="goToMapFragment"
        android:drawableTop="@drawable/settings"
        android:textColorLink="#1e90ff"
        app:icon="@drawable/settings"
        app:iconSize="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/auto_nav4"
        android:layout_width="109dp"
        android:layout_height="109dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/round_button"
        android:onClick="goToMapFragment"
        android:drawableTop="@drawable/joystick"
        app:iconSize="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/auto_nav3"
        android:layout_width="109dp"
        android:layout_height="109dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/round_button"
        android:onClick="goToMapFragment"
        android:drawableTop="@drawable/robotarm"
        app:iconSize="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/auto_nav"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/round_big_button"
        android:onClick="goToMapFragment"
        android:drawableTop="@drawable/icons8"
        android:padding="30dp"
        app:iconSize="120dp"
        app:layout_constraintBottom_toTopOf="@id/auto_nav3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/auto_nav2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/auto_nav5"
        android:layout_width="109dp"
        android:layout_height="109dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/round_button"
        android:onClick="goToMapFragment"
        android:drawableTop="@drawable/smarthome"
        android:textColorLink="#FFEB3B"
        app:icon="@drawable/gps"
        app:iconSize="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/home_page_title"
        android:layout_width="261dp"
        android:layout_height="121dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="left"
        android:lineSpacingExtra="-7dp"
        android:text="Wheelchair Dashboard"
        android:textColor="#F9F9F9"
        android:textSize="40sp"
        android:typeface="sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.191"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/home_page_title3"
        android:layout_width="114dp"
        android:layout_height="115dp"
        android:layout_marginTop="12dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="left"
        android:lineSpacingExtra="-7dp"
        android:text="13"
        android:textColor="#F9F9F9"
        android:textSize="80sp"
        android:typeface="sans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="272dp"
        android:layout_marginTop="108dp"
        android:text="AUGUST 2021"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="108dp"
        android:text="Where to go today?"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>